<form method="post" action="{{ path('admin_categorie_delete', {'id': categorie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this category?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.id) }}">
	<button class="btn btn-outline-light bg-dark">Delete</button>
</form>
